ADMIN_PASS=${ADMIN_PASS:-"admin"}
ADMIN_USER=${ADMIN_USER:-"admin"}
COMPANY_NAME=${COMPANY_NAME:-"Example Company"}
DB_PORT=${DB_PORT:-3306}
DEFAULT_COLOR=${DEFAULT_COLOR:-"1b75bb"}
ENABLE_AUTO_UPDATE=${ENABLE_AUTO_UPDATE:-"TRUE"}
LANGUAGE=${LANGUAGE:-"en-US"}
LOGO_FILE=${LOGO_FILE:-"logo.png"}
LOGO_PATH=${LOGO_PATH:-"/images/"}
SESSION_EXPIRE=${SESSION_EXPIRE:-"86400"}
STORAGE_TYPE=${STORAGE_TYPE:-"FILESYSTEM"}
STORAGE_FILE_PATH=${STORAGE_FILE_PATH:-"userfiles"}
SMTP_ENABLE=${SMTP_ENABLE:-"FALSE"}
SMTP_TLS_AUTO=${SMTP_TLS_AUTO:-"TRUE"}
SETUP_TYPE=${SETUP_TYPE:-"AUTO"}
SITE_NAME=${SITE_NAME:-"Leantime"}
